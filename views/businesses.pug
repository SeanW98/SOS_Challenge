include navbar

doctype html
html
  head
    title= title
block content
    h1(id="businessPage") #{type}
    p(id="results") All Results 
        ul(id="businessesList")
            each business, i in businesses
                div(class="businessBox")
                    img(src="/images/tempPhoto.png" id="businessesPhoto")
                    a(href= "/businesses/" + type + "/" + business._id , class='tourismList' )= business.businessName
                br
                br

